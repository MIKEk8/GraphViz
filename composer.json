{
    "name": "phpdocumentor/graphviz",
    "description": "Wrapper for Graphviz",
    "type": "library",
    "license": "MIT",
    "authors": [
        {
            "name": "Mike van Riel",
            "email": "mike.vanriel@naenius.com"
        }
    ],
    "require": {
        "php": "^7.1"
    },
    "autoload": {
        "psr-4": {
            "phpDocumentor\\GraphViz\\": "src/phpDocumentor/GraphViz"
        }
    },
